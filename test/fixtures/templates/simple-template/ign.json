{
  "name": "simple-template",
  "version": "1.0.0",
  "description": "A simple test template",
  "variables": {
    "project_name": {
      "type": "string",
      "description": "Project name",
      "required": true
    },
    "port": {
      "type": "int",
      "description": "Server port",
      "required": false,
      "default": 8080
    },
    "enable_feature": {
      "type": "bool",
      "description": "Enable optional feature",
      "required": false,
      "default": false
    }
  }
}
