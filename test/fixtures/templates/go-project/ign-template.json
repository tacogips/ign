{
  "name": "go-project",
  "version": "1.0.0",
  "description": "Go project template",
  "variables": {
    "module_name": {
      "type": "string",
      "description": "Go module name",
      "required": true,
      "pattern": "^[a-z0-9-]+(/[a-z0-9-]+)*$"
    },
    "author": {
      "type": "string",
      "description": "Author name",
      "required": true
    }
  }
}
